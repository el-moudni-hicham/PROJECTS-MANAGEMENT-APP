<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="root" prefHeight="558.0" prefWidth="855.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ma.enset.projet.presentation.controllers.admin.projects.ProjectsController">
    <children>
        <VBox prefHeight="558.0" prefWidth="917.0" spacing="30.0">
            <children>
                <Label prefHeight="33.0" prefWidth="271.0" text="Projects">
                    <font>
                        <Font name="Corbel Bold" size="28.0" />
                    </font>
                </Label>
                <VBox prefHeight="39.0" prefWidth="708.0">
                    <children>
                        <Button fx:id="addProject" mnemonicParsing="false" onAction="#addProject" prefHeight="31.0" prefWidth="128.0" style="-fx-background-color: F9A80C; -fx-background-radius: 50;" text="Ajouter Project">
                            <font>
                                <Font name="Corbel" size="15.0" />
                            </font>
                            <VBox.margin>
                                <Insets left="30.0" />
                            </VBox.margin>
                        </Button>
                    </children>
                </VBox>
                <TableView prefHeight="326.0" prefWidth="780.0">
                    <columns>
                        <TableColumn prefWidth="75.0" text="ID" />
                        <TableColumn prefWidth="341.0" text="Nom" />
                        <TableColumn prefWidth="400.0" text="Responsable" />
                    </columns>
                </TableView>
            </children>
            <padding>
                <Insets bottom="30.0" left="50.0" right="50.0" top="30.0" />
            </padding>
        </VBox>
    </children>
</AnchorPane>
